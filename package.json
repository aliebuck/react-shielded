{
  "name": "react-shielded",
  "version": "2.1.0",
  "description": "Add the Women's Refuge Shielded Site button to your React website",
  "type": "module",
  "main": "./dist/react-shielded.cjs",
  "module": "./dist/react-shielded.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-shielded.js",
      "require": "./dist/react-shielded.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "npm run build && husky",
    "preversion": "npm run lint && npm run coverage",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliebuck/react-shielded.git"
  },
  "keywords": [
    "react",
    "component",
    "shielded",
    "nz",
    "Womans Refuge",
    "New Zealand"
  ],
  "author": "Alie Buck <830470+aliebuck@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliebuck/react-shielded/issues"
  },
  "homepage": "https://github.com/aliebuck/react-shielded#readme",
  "dependencies": {
    "react-document-portal": "^1.2.1",
    "react-use-keypress": "^1.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/eslint-plugin": "^1.5.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-testing-library": "^7.13.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "react": "^16.14.0 || 17 || 18",
    "react-dom": "^16.14.0 || 17 || 18"
  }
}
